PALETTEDICTIONARY	"tile_dictionary";

IGNORECOLORS ((255, 0, 0), (1, 1, 1));

SOURCE "temp_sheet_ground";
SPRITESET "set_ground", (10, 12);
	MAPPEDCOLORS	"tiles_default", ("grass_flower");
	MULTIPLE COLOR "", ("grass_flower", "grass_flower_alt"), (0, 0), (0, 0), (4, 1); END;
	
	MAPPEDCOLORS	"tiles_default", ("leaves");
	MULTIPLE BASIC (0, 1), (0, 1), (4, 1); END;
	
	MAPPEDCOLORS	"tiles_default", ("grass_field");
	MULTIPLE STYLE "grass_ground", (0, 2), (2, 1);
		ADD "two_strands"			(0, 2);
		ADD "three_strands"			(0, 3);
		ADD "cluttered"				(0, 4);
		ADD "groomed"				(0, 5);
		END;
		
	MULTIPLE STYLE "grass_green", (2, 2), (2, 1);
		ADD "two_strands"			(2, 2);
		ADD "three_strands"			(2, 3);
		ADD "cluttered"				(2, 4);
		ADD "groomed"				(2, 5);
		END;
		
	MAPPEDCOLORS	"tiles_default", ("dirt");
	MULTIPLE COLOR "", ("dirt", "dirt_alt"), (0, 8), (0, 3), (2, 2); END;
	MULTIPLE COLORSTYLE "dirt_rough", "", ("dirt", "dirt_alt"), (2, 3), (2, 2);
		ADD "ages"					(0, 6);
		ADD "seasons"				(2, 6);
		END;
		
	MAPPEDCOLORS	"tiles_default", ("grass_border");
	MULTIPLE COLORSTYLE "ground_border", "", ("grass_border", "sand"), (4, 0), (3, 3);
		ADD "light"					(4, 0);
		ADD "dark"					(4, 3);
		END;
		
	MULTIPLE COLOR "", ("grass_border", "sand"), (4, 9), (4, 6), (2, 2); END;
		
	MAPPEDCOLORS	"tiles_default", ("dirt");
	MULTIPLE COLORSTYLE "ground_border", "", ("dirt", "dirt_alt"), (7, 0), (3, 3);
		ADD "light"					(7, 0);
		ADD "dark"					(7, 3);
		END;
		
	MAPPEDCOLORS	"tiles_default", ("grass_field");
	MULTIPLE BASIC (4, 6), (4, 3), (3, 3); END;
	SINGLE BASIC (6, 10), (6, 7); END;
	SINGLE COLOR "", ("grass_field", "sand"), (6, 9), (6, 6); END;
	
	MAPPEDCOLORS	"tiles_default", ("leaves");
	MULTIPLE BASIC (4, 11), (4, 8), (2, 1); END;
	
	MAPPEDCOLORS	"tiles_default", ("dirt");
	MULTIPLE COLOR "", ("dirt", "dirt_alt") (7, 6), (7, 3), (3, 3); END;
	MULTIPLE COLOR "", ("dirt", "dirt_alt") (7, 9), (7, 6), (2, 2); END;
	SINGLE COLOR "", ("dirt", "dirt_alt") (9, 11), (9, 7); END;
	SINGLE COLORSTYLE "dirt", "", ("dirt", "dirt_alt"), (9, 6);
		ADD "overworld"				(9, 9);
		ADD "interior"				(9, 10);
		END;
END;

