ANIMATION "tile_grass_flowers"; SUBSTRIP repeat;
	ADD frame, 16, ((3, 0), "grass_flower"), (0, 0);
	ADD frame, 16, ((0, 0), "grass_flower"), (0, 0);
	ADD frame, 16, ((1, 0), "grass_flower"), (0, 0);
	ADD frame, 16, ((2, 0), "grass_flower"), (0, 0);
	INSERT strip, 0, 16, 4, ((0, 0), "grass_flower"), (8, 8);
	ADD static, "grass_green_8x8_right", (8, 0);
	ADD static, "grass_green_8x8_left", (0, 8);
	END;
	
